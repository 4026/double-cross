{% extends "Four026CabinetBundle:Default:cover.html.twig" %}

{% block circle_content %}

    <h3>Register</h3>

    {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.email_address) }}
        {{ form_row(form.password) }}
        {{ form_row(form.register, { 'attr': {'class': 'btn-primary'} }) }}
    {{ form_end(form) }}

{% endblock %}