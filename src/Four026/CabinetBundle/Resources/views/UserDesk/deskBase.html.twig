{% extends "Four026CabinetBundle:Default:bootstrap_base.html.twig" %}
{% import "Four026CabinetBundle:Default:macros.html.twig" as macros %}


{% block javascripts %}
    <script type="text/javascript" src='//cdnjs.cloudflare.com/ajax/libs/jQuery.dotdotdot/1.7.2/jquery.dotdotdot.min.js'></script>
    <script type="text/javascript" src='/zeroclipboard/ZeroClipboard.min.js'></script>
    {% javascripts '@Four026CabinetBundle/Resources/public/js/list_documents.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block stylesheets %}
    {% stylesheets
        '@Four026CabinetBundle/Resources/public/css/casefiles.css'
        filter='cssrewrite'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block main_content %}

<div class="jumbotron">
    <div class="container">
        {% block jumbotron_content %}
        {% endblock %}
    </div>
</div>

<div class='container'>
    {% block main_container %}
    {% endblock %}
</div>
{% endblock %}
